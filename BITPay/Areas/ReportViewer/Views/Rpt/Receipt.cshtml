@model ReceiptReportModels
@{
    Layout = "_LayoutReceipt";
}

<br/>
    <table style="width:100%">
        <tr>
            <td><div style="text-align:right;"><h4 style="font:bold;">RECU DE PAIEMENT</h4></div></td>
            <td>
                <div style="text-align:right;"><img class="logo" src="~/images/ObrLogo.png" alt=".." /> </div>
            </td>
        </tr>


    </table>
<table style="width:100%;font-size:10px">
    <tr>
        <td style="text-align:left;">
            <table style="width:100%;">
                <tr>
                    <td><strong>Quittance No:</strong> @Model.OBRReceiptNo</td>
                    <td style="text-align: right;"><strong>Du:</strong> @Model.ReceiptDate.ToString("dd/MM/yyyy hh:mm tt")</td>
                </tr>
            </table>
        </td>
    </tr>
    <tr>
        <td style="text-align:left;">
            <strong> Bureau de douane: </strong> @Model.OfficeCode-@Model.OfficeName
        </td>
    </tr>
    <tr>
        <td style="text-align:left;">
            <strong>Délivrée à:</strong> @Model.BranchName
        </td>
    </tr>
    <tr>
        <td style="text-align:left;">
            <strong>Entreprise:</strong> @Model.CompanyCode - @Model.CompanyName
        </td>
    </tr>

    <tr>
        <td>

            <table style="width:100%;">
                <tr>
                    <td><strong>Déclarant:</strong> @Model.DeclarantCode -  @Model.DeclarantName</td>
                    <td style="text-align: right;"><strong>Déposant:</strong>  @Model.ReceivedFrom </td>
                </tr>
            </table>
        </td>
    </tr>
</table>
<table class="table  table-striped" style="width:100%;font-size:12px">
    <tr>
        <td> <strong>Référence du déclarant </strong></td>
        <td> <strong>Référence déclaration</strong></td>
        <td> <strong>Modéle </strong></td>
        <td> <strong>Montant payé </strong></td>

    </tr>
    <tr>
        <td></td>
        <td>@Model.RegYear-@Model.AsmtSerial-@Model.AsmtNumber</td>
        <td></td>
        <td>@Model.Amount.ToString("#,##0.00")</td>
    </tr>
    <tr><td></td> <td><strong> Total à encaisser pour les Déclarations </strong></td><td></td> <td> @Model.Amount.ToString("#,##0.00")</td></tr>
    <tr>

        <td><strong>Moyen de paiement</strong></td>
        <td><strong> Référence </strong></td>
        <td><strong>Banque </strong></td>
        <td><strong>Montant payé </strong></td>
    <tr />
    <tr>
        <td>@Model.PaymentMode</td>
        <td>@Model.Extra3</td>
        <td>Finbank SA</td>
        <td>@Model.Amount</td>
    </tr>
    @*<tr><td></td> <td style="text-align:right;"> <strong>Total par moyens de paiement  </strong></td><td></td>  <td>@Model.Amount.ToString("#,##0.00")</td></tr>*@
    <tr><td></td> <td style="text-align:right;"> <strong>Montant total encaissé  </strong></td><td></td>  <td> @Model.Amount.ToString("#,##0.00")</td></tr>
</table>
@*<p class="text-center">
    Finbank SA Reconnait avoir encaisse la somme ci-dessus.
</p>*@

