@model CreateFileResultModel
@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="shortcut icon" href="~/images/favicon.ico" type="image/x-icon">
    <link rel="icon" href="~/images/favicon.ico" type="image/x-icon">

    <title>Report Error|BITPay</title>
    <link href="~/css/imports.css" rel="stylesheet" />
    <link href="~/css/site.min.css" rel="stylesheet" />
</head>
<body>
    <div style="padding:20px;">
        <h3>BITPay</h3>
        <div class="card-box">
            <h4 class="text-danger"><i class="fa fa-warning"></i> Report Error!</h4>
            <div class="alert alert-danger">
                @Model.Message
            </div>
            <p>You can check the error log for more details.</p>
        </div>
    </div>
</body>
</html>