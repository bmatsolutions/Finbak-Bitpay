@model PaymentDetails


<div class="card-box">
    <div class="row">
        <div class="table-responsive">

            <table class="table table-striped table-bordered table-responsive-sm nowrap" id="Data" style="width:100%">
                <thead>
                    <tr>
                        <th class="header">Txn Ref</th>
                        <th class="header">Customer Name</th>
                        <th class="header">Date of Payment</th>
                        <th class="header">Status</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td>@Model.code</td>
                        <td>@Model.cusName</td>
                        <td>@Model.date</td>
                        <td>@Model.tranStat</td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
</div>
